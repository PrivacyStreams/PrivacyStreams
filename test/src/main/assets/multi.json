{
  "interval": 1000,
  "Audio": {"duration" : 1000,
            "features": [
              {"operatorName": "AudioOperators.calcLoudness",
                "operatorParams": ["audio_data"],
               "featureName": "loudness"},
              {"operatorName": "ArithmeticOperators.add",
                "operatorParams": ["loudness", "loudness"],
                "featureName": "doubleLoudness"}
            ]},
  "Light": {"sensorDelay" : 1000,
            "features":[
              {"operatorName": "MultiOperators.getField",
                "operatorParams":["illuminance"],
               "featureName": "illuminance"}
            ]},
  "Gyroscope": {"sensorDelay" : 1000,
                "features":[
                  {"operatorName":"MultiOperators.getField",
                    "operatorParams":["x"],
                    "featureName":"x"},
                  {"operatorName":"MultiOperators.getField",
                    "operatorParams":["y"],
                  "featureName":"y"},
                  {"operatorName":"MultiOperators.getField",
                    "operatorParams":["z"],
                  "featureName":"z"}
                ]}
}